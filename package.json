    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "http-errors": "^2.0.0",
    "inversify": "^6.1.1",
    "mysql2": "^3.11.3",
    "reflect-metadata": "^0.2.2",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
