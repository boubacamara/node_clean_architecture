  "main": "dist/src/app.js",
  "scripts": {
    "dev": "nodemon",
    "model:generate": "ts-node ./node_modules/.bin/sequelize model:generate --name",
    "migration:create": "ts-node ./node_modules/.bin/sequelize migration:create --name",
    "migration:generate": "ts-node ./node_modules/.bin/sequelize migration:generate --name",
    "seed:create": "ts-node ./node_modules/.bin/sequelize seed:create --name",
    "seed:generate": "ts-node ./node_modules/.bin/sequelize seed:generate --name",
    "db:create": "ts-node ./node_modules/.bin/sequelize db:create",
    "db:migrate": "ts-node ./node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "ts-node ./node_modules/.bin/sequelize db:migrate:undo",
    "db:migrate:undo:all": "ts-node ./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seed": "ts-node ./node_modules/.bin/sequelize db:seed:all --to",
    "db:seed:undo": "ts-node ./node_modules/.bin/sequelize db:seed:all:undo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
